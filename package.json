{
  "name": "jupyterlab-log-renderer",
  "version": "0.1.0",
  "description": "A JupyterLab extension to render rich-display objects to the JupyterLab log window.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/agoose77/jupyterlab-log-renderer",
  "bugs": {
    "url": "https://github.com/agoose77/jupyterlab-log-renderer/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Angus Hollands",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/agoose77/jupyterlab-log-renderer.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "prepare": "jlpm run clean && jlpm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^1.2.1",
    "@jupyterlab/logconsole": "^1.0.2",
    "@jupyterlab/notebook": "^1.2.2"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "typescript": "~3.5.2"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "extension": true
  }
}
